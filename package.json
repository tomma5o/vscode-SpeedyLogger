{
    "name": "vscode-speedylogger",
    "displayName": "Speedy Logger",
    "description": "Help insert and remove console log statements",
    "version": "1.0.0",
    "publisher": "tomma5o",
    "repository": "https://github.com/tomma5o/vscode-speedyLogger",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Debuggers"
    ],
    "activationEvents": [
        "onCommand:extension.insertLogStatement",
        "onCommand:extension.deleteAllLogStatements"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.insertLogStatement",
                "title": "Insert Log Statement"
            },
            {
                "command": "extension.deleteAllLogStatements",
                "title": "Insert Log Statement"
            }
        ],
        "keybindings": [
            {
                "command": "extension.insertLogStatement",
                "key": "shift+ctrl+l",
                "mac": "shift+cmd+l"
            },
            {
                "command": "extension.deleteAllLogStatements",
                "key": "shift+ctrl+d",
                "mac": "shift+cmd+d"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "eslint": "^3.6.0",
        "esprima": "^4.0.0",
        "mocha": "^2.3.3",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {}
}
